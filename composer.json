{
    "name": "jp3cki/totp",
    "type": "library",
    "description": "RFC 6238 / TOTP: Time-Based One-Time Password Algorithm",
    "keywords": ["totp", "otp", "rfc6238", "onetime", "password"],
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@bouhime.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "php-64bit": ">=5.4.0",
        "ext-hash": "*",
        "christian-riesen/base32": "^1.2"
    },
    "require-dev": {
        "apigen/apigen": "^4.1",
        "codeclimate/php-test-reporter": "dev-master",
        "phpmd/phpmd": "^2.4",
        "phpunit/phpunit": "^4.8|^5.3",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "suggest": {
        "paragonie/random_compat": "The random_bytes() polyfill library for PHP 5.6 or earlier.",
        "ext-openssl": "Pseudo random bytes will generate with OpenSSL function if your system has not random_bytes() and /dev/urandom."
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\totp\\": "src"
        }
    }
}
